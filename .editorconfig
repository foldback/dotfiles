# macOS
# .editorconfig

# top-most EditorConfig file
root = true

# Apply to all files
[*]

# Set default charset
charset = utf-8

# 2 space indentation
indent_style = space
indent_size = 2

# Unix-style newlines with a newline ending every file
end_of_line = lf
insert_final_newline = true

# Trim whitespace
trim_trailing_whitespace = true
